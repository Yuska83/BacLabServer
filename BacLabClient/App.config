<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <system.web>
    <compilation debug="true"/>
  </system.web>
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="ep1"  >
          <security mode="None"/>
          <reliableSession enabled="true" />
        </binding>
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint name="ep1"
        address="net.tcp://localhost/BacLabServer/ep1" 
        binding="netTcpBinding"
        bindingConfiguration="ep1" 
        contract="ServiceReference1.ITest"
        >
        <identity>
          <userPrincipalName value="BAKLAB\Ифслдфи" />
        </identity>
      </endpoint>
    </client>


  </system.serviceModel>
</configuration>